# Feedback Analyzer Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit .env file to source control

# ===== API Configuration =====

# Database Connection
ConnectionStrings__DefaultConnection=Server=(localdb)\\mssqllocaldb;Database=FeedbackAnalyzerDb;Trusted_Connection=True;MultipleActiveResultSets=true

# Azure Cognitive Services
AzureCognitiveServices__Endpoint=https://your-resource-name.cognitiveservices.azure.com/
AzureCognitiveServices__ApiKey=your-api-key-here

# CORS Configuration
CorsOrigins=https://localhost:5001,http://localhost:5001

# ===== Web Application Configuration =====

# API Settings
ApiSettings__BaseUrl=https://localhost:7000
ApiSettings__HubUrl=https://localhost:7000/feedbackhub

# ===== Environment =====
ASPNETCORE_ENVIRONMENT=Development
